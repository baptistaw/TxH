{
  "name": "txh-registro-backend",
  "version": "1.0.0",
  "description": "Backend API - Sistema Registro Anestesiol√≥gico TxH (JavaScript)",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "prisma generate",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "prisma:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "format": "prisma format && prettier --write \"src/**/*.js\"",
    "lint": "eslint src --ext .js --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:int": "jest --testPathPattern=tests/integration",
    "test:setup": "./scripts/test-setup.sh",
    "test:ci": "npm run lint && npm test",
    "etl:full": "node tools/etl/sheetsToPg.js",
    "etl:incremental": "node tools/etl/incrementalJob.js",
    "etl:cron": "node tools/etl/startCron.js",
    "etl:validate": "node tools/etl/validateMigration.js"
  },
  "keywords": [
    "anesthesia",
    "transplant",
    "medical-records",
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "javascript"
  ],
  "author": "William Baptista <baptistaw@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@clerk/backend": "^2.24.0",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@google-cloud/vision": "^5.3.4",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.8.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^3.0.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^166.0.0",
    "helmet": "^7.1.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.10",
    "puppeteer": "^21.7.0",
    "sharp": "^0.34.5",
    "string-similarity": "^4.0.4",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "config"
    ],
    "ext": "js,json",
    "ignore": [
      "src/**/*.test.js",
      "node_modules"
    ],
    "env": {
      "NODE_ENV": "development",
      "PORT": "4000"
    }
  }
}
